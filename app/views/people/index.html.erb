<h1>people</h1>

<table>

<% for person in @people %>
  <tr>
    <td><%=h person.first_name %></td>
    <td><%=h person.middle_name %></td>
    <td><%=h person.last_name %></td>
    <td><%= link_to_name person.user %></td>
    <td><%= link_to 'show', person %></td>
  </tr>
<% end %>
</table>

<br />

<%= scoped_new_link :person %>
