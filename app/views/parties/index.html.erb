<h1>parties</h1>

<table>
  <tr>
    <th>Time</th>
    <th>Until</th>
    <th>Target</th>

  </tr>

<% for party in @parties %>
  <tr>
    <td><%=h party.time.strftime("%I:%M %A %m/%d") %></td>
    <td><%=h party.until.strftime("%I:%M %p") %></td>
    <td><%= link_to_name party.target %></td>

    <td><%= link_to 'show', party %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New party', new_party_path %>
