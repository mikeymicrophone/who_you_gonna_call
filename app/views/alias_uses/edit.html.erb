<h1>Editing alias_use</h1>

<% form_for(@alias_use) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :alias_id %><br />
    <%= f.text_field :alias_id %>
  </p>
  <p>
    <%= f.label :target_type %><br />
    <%= f.text_field :target_type %>
  </p>
  <p>
    <%= f.label :target_id %><br />
    <%= f.text_field :target_id %>
  </p>
  
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'show', @alias_use %> |
<%= back_link %>
