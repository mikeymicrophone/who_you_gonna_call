<h1>aliases</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Service</th>

  </tr>

<% for alias in @aliases %>
  <tr>
    <td><%=h alias.name %></td>
    <td><%= link_to_name alias.service %></td>

    <td><%= link_to 'show', alias %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New alias', new_alias_path %>
